{
    "name": "always-open/laravel-process-stamps",
    "description": "Logs which process created or modified a record",
    "keywords": [
        "always open",
        "always-open",
        "laravel-process-stamps",
        "laravel",
        "logging"
    ],
    "homepage": "https://github.com/always-open/laravel-process-stamps",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Schlick",
            "role": "Developer"
        },
        {
            "name": "Quentin Schmick",
            "email": "qschmick@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Lucas Graciano",
            "role": "Developer"
        },
        {
            "name": "Always Open",
            "homepage": "https://github.com/always-open",
            "role": "Organization"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0|^9.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.9|^3.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "AlwaysOpen\\ProcessStamps\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlwaysOpen\\ProcessStamps\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlwaysOpen\\ProcessStamps\\ProcessStampsServiceProvider"
            ]
        }
    }
}
